1.Insert data jurusan pada tabel tblJurusan
  INSERT INTO tblJurusan VALUES('J1','Teknik Informatika');
  INSERT INTO tblJurusan VALUES('J2','Sistem Informasi');
  INSERT INTO tblJurusan VALUES('J3','Komputer Grafik');

2.Tampilakan seluruh data tblJurusan
  SELECT * FROM tblJurusan; 

3.Tampilkan data kolom nama_jur pada tabel jurusan
  SELECT nama_jur from tblJurusan;
4.Insert data mahasiswa
  INSERT INTO mahasiswa VALUES 
  ('311610610','Jaki','jax@gmail.com','08132222','Bekasi','J1');

